{
    "manifest_version": 3,
    "name": "YGG x Deluge",
    "version": "1.0",
    "description": "Une extension qui permet d'envoyer directement le torrent dans deluge sans le télécharger.",
    "host_permissions": ["<all_urls>"],
    "permissions": [
      "activeTab",
      "scripting",
      "tabs",
      "cookies"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.ygg.re/*"],
        "js": ["content.js"]
      }
    ]
  }
  